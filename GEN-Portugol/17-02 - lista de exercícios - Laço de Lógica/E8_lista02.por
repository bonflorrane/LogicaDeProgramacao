programa
{
	
	funcao inicio()
	{
		real N
		
		escreva("Digite um número: ")
		leia(N)
		se(N>100)
		{
			escreva("O número digitado é: ", N)
		}
		senao
		{
			escreva("O valor atribuido ao número é zero, N = 0")
		}
	}
}
